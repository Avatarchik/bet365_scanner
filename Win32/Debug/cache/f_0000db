function getmoneychg(a)
{
	//alert(a);
	if(a=='1')
	{
 jQuery('#getm01').show();
 jQuery('#getm02').hide();
 jQuery('#getmoneychg_0').removeClass();
 //jQuery('#getmoneychg_1').removeClass();
 jQuery('#getmoneychg_0').addClass('getmoneychg_00');
 //jQuery('#getmoneychg_1').addClass('getmoneychg_11');		
  jQuery('#bankaccountid').val('1');
	}
	else
	{
 jQuery('#getm01').hide();
 jQuery('#getm02').show();
 
 jQuery('#getmoneychg_0').removeClass();
 jQuery('#getmoneychg_1').removeClass();
 jQuery('#getmoneychg_0').addClass('getmoneychg_0_c');
 jQuery('#getmoneychg_1').addClass('getmoneychg_1_c');
 jQuery('#bankaccountid').val('2');
	}
}
function cashsave(a)
{
	if(a=='1')
	{
		
 jQuery('#suddenid0').removeClass();
 jQuery('#suddenid1').removeClass();
 jQuery('#suddenid0').addClass('sudden0');
 jQuery('#suddenid1').addClass('sudden1');		
 jQuery('#savemoneychg_01').val('1');
	}
	else
	{
		jQuery('#suddenid0').removeClass();
    jQuery('#suddenid1').removeClass();
    jQuery('#suddenid0').addClass('sudden00');
    jQuery('#suddenid1').addClass('sudden11');
	  jQuery('#savemoneychg_01').val('2');
	}
}

