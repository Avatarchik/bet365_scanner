var isAltF4 = true;
jQuery(document).mouseleave(function(e){
    
    window.onbeforeunload =  function () {
        
        //return '您確定要離開？';
          jQuery.cookies.set('wa99', null);
         }
});
jQuery(document).mouseenter(function(e){
    //jQuery('#debug').html('enter')
    window.onbeforeunload =  null
    window.onunload =  null
});

jQuery(function(){  
  
  jQuery('[id^="chk"]').removeClass('current-menu-item');
    var ss = jQuery.cookies.get('wa99');
//alert(ss);
    if(ss==null || ss=='33')
    {
  jQuery('#chk1').addClass('current-menu-item');    	
    }
    if(ss=='1')
    {
  jQuery('#chk'+ss+'').addClass('current-menu-item');
    }
   if(ss=='2')
    {   
 jQuery('#chk'+ss+'').addClass('current-menu-item');   	
    }
   if(ss=='3')
    {
 jQuery('#chk'+ss+'').addClass('current-menu-item');   	
    }
    if(ss=='19')
    {
    	//alert(a);
 jQuery('#chk'+ss+'').addClass('current-menu-item');   	
    }

});

function chkmem(a)
{
  //alert(a);
  if(a=='33')
	 {
    jQuery.cookies.set('wa99', null);
    jQuery.cookies.set('wa99', a);
	 }
	
	if(a=='2')
	 {
 jQuery.cookies.set('wa99', null);
 jQuery.cookies.set('wa99', a);
	 }
	 
  if(a=='3')
	 {
 jQuery.cookies.set('wa99', null);
 jQuery.cookies.set('wa99', a);
	 }
	 
	if(a=='19')
	 {
   jQuery.cookies.set('wa99', null);
   jQuery.cookies.set('wa99', a);
	 }
}

function userinfo(a)
{
	
		  if(a=='33')
		  {
		  	document.location.href='savepickup.php';
		  }
		  
		  else
		  {
		  	document.location.href='savepickup.php?type='+a;
		  }
 
  
  
}


