<script>

function bankinfo_sub(a)
{
	//alert(a);
	 jQuery.ajax({
		    type: 'POST',
		    url: './config/banksub.php',
		    data: 'banksub='+encodeURI(a)+'',		
		    error: function(xhr)
		      {
			   //jQuery('#order').html('<font color=#FF0000>网路忙线中，请稍后再试!错误代码:681</font>');
			    alert('網路忙線中，請稍後再試!錯誤代碼:36');
			    return false;
		      },
			   beforeSend:function(response)
					{   
					},
					
	      success:function(response)
	        {	
			  //alert(response);
			  jQuery('#banksub').html(response);      			             
		      }
    
	  	   });
}

function ChooseCity(a)
{
	var aa = a.split('-');
	 
	 jQuery.ajax({
		    type: 'POST',
		    url: './config/chinacity.php',
		    data: 'chinacity='+aa[0]+'',		
		    error: function(xhr)
		      {
			   //jQuery('#order').html('<font color=#FF0000>网路忙线中，请稍后再试!错误代码:681</font>');
			    alert('網路忙線中，請稍後再試!錯誤代碼:36');
			    return false;
		      },
			   beforeSend:function(response)
					{   
					}, 
	      success:function(response)
	        {		
			jQuery('#provincechinacity').html(response);      
			             
		      }
    
	  	         });
}

function Chooseself(a)
{
	var aa = a.split(':');
  //alert(aa[0]);
  jQuery(":input[id=bankaccount_self]").val();
	jQuery(':input[id=bankaccount_self]').val(aa[0])
	//jQuery( "#bankaccount_self" ).val(aa[0]);
	jQuery( "#bankaccountname" ).val(aa[1]);
	//jQuery( "#bankaccountid" ).val(aa[2]);
	jQuery( ":input[id=bankaccount_self]" ).attr('disabled',true);
}

function checksavepickup_15(a)
{
	if(a=='1')
	{
	 jQuery( "#savemember15_down" ).hide();
	 jQuery("#savemember15_up").show();
	}
	if(a=='2')
	{
	 jQuery( "#savemember15_down" ).show();
	 jQuery("#savemember15_up").hide();
	}
}

