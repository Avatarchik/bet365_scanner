function stopsavepickup(a)
{
	 if(a=='1')
	 {
	 	var ss = '此会员取消提款功能';
	 }
	 if(a=='2')
	 {
	 	var ss = '此会员取消存款功能';
	 }
	    swal({title: "錯誤讯息",text: ss, confirmButtonText: "确定",closeOnConfirm: true, customClass: 'twitter'}, function(){document.location.href='./savepickup.php?type=3'});
	  	return false;
}
