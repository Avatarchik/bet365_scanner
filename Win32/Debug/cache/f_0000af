	}
/*提領選擇交易平台聯營卡,支付寶*/
function checksavepickup(a)
{
	if(a=='1')
	{
	 jQuery( "#Third_party" ).hide();
	 jQuery("#un_bank").show();
	 jQuery(':input[id=pickup1]').css({'background-color':'#FF0000','box-shadow':'1px 1px 1px 1px orange inset'});
	 jQuery(':input[id=pickup2]').css({'background-color':'#FF0000','box-shadow':'0px 0px 0px 0px orange inset'});
	}
	if(a=='2')
	{
	 jQuery( "#Third_party" ).show();
	 jQuery("#un_bank").hide();
	 jQuery(':input[id=pickup2]').css({'background-color':'#FF0000','box-shadow':'1px 1px 1px 1px orange inset'});
	 jQuery(':input[id=pickup1]').css({'background-color':'#FF0000','box-shadow':'0px 0px 0px 0px orange inset'});
	}
}

/*提領送出*/
function unpickupmoney(a)
{
