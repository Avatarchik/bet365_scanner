var globalnum;

function savemember1()
{
	  //var sre = /^[0-9]+(\.[0-9]{0,0})?$/;
	  var reg = /^[0-9]*$/;
	  var sre = /^[0-9]+(\.[0-9]{1,2})?$/;//小數點2位
	  var bankaccountword = jQuery("#bankm").val();
	  var banksubid = jQuery("#banksubid").val();
	  var zz = banksubid.split(':');
	  var line_money = jQuery("#line_money").val();
	  var sa = jQuery("#savemoneychg_01").val();
	  
	 
	  if(sa=='1')
	  {
	  	jQuery("#orderplace_out").show();
	  	jQuery("#orderplace_out_1").show();
	  	
	  }
	  else if(sa=='2')
	  {
	  	jQuery("#orderplace_out").hide();
	  	jQuery("#orderplace_out_1").hide();
	  	jQuery("#orderplace_out_sec tr").css({"height":"220px"});
	  	
	  }
	  
	  if(bankaccountword=='' || line_money=='')
	  {
	  	return false;
	  }
	   else if(line_money < 100)
	   {
	   	swal({title: "错误讯息", text: "存款金额未达100", confirmButtonText: "确定",customClass: 'twitter'});      	
	   	//change_alert('#FF0000','错误讯息','金额于范围内!','#FFFFFF');
	   	//alert('金额于范围内!');
	   	return;
	   }
    
    else if(!reg.test(line_money))
	    {
	  //swal({title: "错误讯息", text: "存款金额最多小数点2位!", confirmButtonText: "确定",customClass: 'twitter'});
	  swal({title: "错误讯息", text: "充值请输入整数!", confirmButtonText: "确定",customClass: 'twitter'});      	
	  //change_alert('#FF0000','错误讯息','儲值請輸入整數!','#FFFFFF');
	  //alert('儲值請輸入整數'); 
	  return;
	    }
	  
	  else
	  	{
	  	}
	 //var nu = Math.floor((Math.random() * 5) + 1);//自動選號
   //var ff = chgwa99bankname(nu);
  //alert(zz[5]+':'+zz[6]);
  //var mun = Math.floor((Math.random() * 100) + 1);
  
  //alert(line_money);
  jQuery('#savemember_bank').html(zz[3]);
  jQuery('#savemember_bankchild').html(zz[4]);
  jQuery('#savemember_acount').html(zz[1]);
  jQuery('#savemember_num').html(zz[2]);
  jQuery('#savemember_urlss').val(zz[5]+':'+zz[6]);
  jQuery('#bankfid').val(zz[0]);
  jQuery('#savemember_menoy').html(line_money);
  jQuery('#savemember1_up').hide();
  jQuery('#savemember1_down').show();
  
  globalnum = nu;
}

function savemember1_orther()
{
	  var sre = /^[0-9]+(\.[0-9]{0,0})?$/;
	  var reg = /^[0-9]*$/;
	  
	  var bankid = jQuery("#bankid").val();
	  var line_money = jQuery("#line_money").val();
	  var content = jQuery("#content").val();
	  
	  if(bankid=='' || line_money=='')
	  {
	  	return false;
	  }
	   else if(line_money < 100)
	   {
	   	swal({title: "错误讯息", text: "存款金额未达100", confirmButtonText: "确定",customClass: 'twitter'});      	
	    //change_alert('#FF0000','错误讯息','金额于范围内!','#FFFFFF');
	   	//alert('金额于范围内!');
	   	return;
	   }
    else if(!sre.test(line_money))
	    {
	  swal({title: "错误讯息", text: "儲值請輸入整數!", confirmButtonText: "确定",customClass: 'twitter'});      	
	  //change_alert('#FF0000','错误讯息','儲值請輸入整數!','#FFFFFF');
	  //alert('儲值請輸入整數'); 
	  return;
	    }
	  
	  else
	  	{
	  	}
	 var nu = Math.floor((Math.random() * 5) + 1);//自動選號
   var ff = chgwa99bankname(nu);
  //alert(ff[nu].bankname);
  
  jQuery('#savemember_bank').html(ff[nu].bankname);
  jQuery('#savemember_bankchild').html(ff[nu].subname);
  jQuery('#savemember_acount').html(ff[nu].person);
  jQuery('#savemember_num').html(ff[nu].account);
  
  jQuery('#savemember_menoy').html(line_money);
  jQuery('#savemember_bankid').html(bankid);
  jQuery('#savemember_content').html(content);
  jQuery('#savemember1_up').hide();
  jQuery('#savemember1_down').show();
  
  globalnum = nu;
}


