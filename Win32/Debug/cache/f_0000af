/*判別wa99在中活各銀行帳號與姓名*/
function chgwa99bankname(a)
{
var result="";

	   jQuery.ajax({
		    type: 'POST',
		    url: './config/wa99bankname.php',
		    data: 'num='+a+'',
		    async: false, //啟用同步請求		
		    error: function(xhr)
		      {
			   //jQuery('#order').html('<font color=#FF0000>网路忙线中，请稍后再试!错误代码:681</font>');
			    alert('網路忙線中，請稍後再試!錯誤代碼:save255');
			    return false;
		      },
			   beforeSend:function(response)
					{   
					}, 
	      success:function(response)
	        {					   
			   
			   //var newbetss = JSON.parse(response);
         //return newbetss[1].bankname;
         result = JSON.parse(response);
		      }
        
	   });
	   
    return result;
	 
}

/*個人資料修改*/
function changepickup(a)
{
	//alert(a);
	  var simple_ar = Array();
  	var c2bet_st = new Object;
  	var orderinfo_st = new Object;
    var re2 = /(?!^[0-9]{6,}$)(?!^[a-zA-Z]{6,}$)^[0-9a-zA-Z]{6,}$/;
	  var reg = /^[a-zA-Z]{2,}/;
   
   if(a=='')
  	{
  		
	    
	    var oldaccount = jQuery("#oldaccount").val();
	    var newaccount = jQuery("#newaccount").val();
	    var renewaccount = jQuery("#renewaccount").val();
	    
  		if(oldaccount=="" || newaccount=="" || renewaccount=="")
	    {
	    	swal({title: "错误讯息", text: "请填写相关密码资讯!", confirmButtonText: "确定",customClass: 'twitter'});
	    	//change_alert('#FF0000','错误讯息','請填寫相關密碼資訊!','#FFFFFF');
	    	//alert('請填寫相關密碼資訊');
	    	return;
	    }
	    else if(newaccount!=renewaccount)
	    {
	    	swal({title: "错误讯息", text: "确认密码错误!", confirmButtonText: "确定",customClass: 'twitter'});
	    	
	    	//change_alert('#FF0000','错误讯息','确认密码错误!','#FFFFFF');
	    	//alert('确认密码错误!');
	    	return;
	    }
  		else if(jQuery.trim(oldaccount.replace(/\s+/g,""))!==oldaccount)
	    {
	    	swal({title: "错误讯息", text: "帐号密码密码错误!", confirmButtonText: "确定",customClass: 'twitter'});
	    	
	    	//change_alert('#FF0000','错误讯息','旧帐号密码請勿有空格','#FFFFFF');
	    	//alert('旧帐号密码請勿有空格');
	    	return;
	    }
	    else if(jQuery.trim(newaccount.replace(/\s+/g,""))!==newaccount)
	    {
	    	swal({title: "错误讯息", text: "新帐号密码错误!", confirmButtonText: "确定",customClass: 'twitter'});
	    	
	    	//change_alert('#FF0000','错误讯息','新帐号密码請勿有空格','#FFFFFF');
	    	//alert('新帐号密码請勿有空格');
	    	return;
