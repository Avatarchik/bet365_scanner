function chgpasswd(a,b)
{
	if(a=='pickup')
	{
ss = '<input class=pickup_edit_password type=password id=oldaccount'+a+' name=oldaccount'+a+' placeholder="输入原提款密码">';
ss1 ='<input class=pickup_edit_password type=password id=newaccount'+a+' name=newaccount'+a+' placeholder="提款密码与会员密码不能相同">';
ss2 = '<input class=pickup_edit_password type=password id=renewaccount'+a+' name=renewaccount'+a+' placeholder="再次输入提款密码">';
ss3 = '<button class=btn_refill_orders type=submit onclick=changepickup("'+a+'")>提交</button><button class=btn_cancel_orders type=button onclick=cancel()>取消</button>';
cont = '<table style="font-size:16px;height:300px;width:100%;color:#ffffff;">';
cont =cont+'<tr><td colspan=2 class=pickup_title>提款密码修改</td></tr>';
cont =cont+'<tr><td class=pickup_edit_pic>原提款密码</td><td class=pickup_edit_passwd>'+ss+'</td></tr>';
cont =cont+'<tr><td class=pickup_edit_pic>提款新密码</td><td class=pickup_edit_passwd>'+ss1+'</td></tr>';
cont =cont+'<tr><td class=pickup_edit_pic>确认新密码</td><td class=pickup_edit_passwd>'+ss2+'</td></tr>';
cont =cont+'<tr><td colspan=2 style=text-align:center;padding-top:15px;>'+ss3+'</td></tr></table>';
	
	}
	else
	{
ss = '<input class=pickup_edit_password type=password id=oldaccount'+a+' name=oldaccount'+a+'>';
ss1 ='<input class=pickup_edit_password type=password id=newaccount'+a+' name=newaccount'+a+' placeholder="密码請英數混合,大於6個字">';
ss2 = '<input class=pickup_edit_password type=password id=renewaccount'+a+' name=renewaccount'+a+' placeholder="再次输入密码">';
ss3 = '<button class=btn_refill_orders type=submit onclick=changepickup("'+a+'")>提交</button><button class=btn_cancel_orders type=button onclick=cancel()>取消</button>';
//ss3 = '<input style="color:#000000;" class=surmobile type=button onclick=changepickup("'+a+'") value=提交><input style="color:#000000;" class=re_surmobile type=button onclick=cancel() value=取消>';
cont = '<table class="passwdagain" style="font-size:16px;height:300px;width:100%;color:#ffffff;">';
cont =cont+'<tr><td colspan=2 class=pickup_title>登入密码修改</td></tr>';
cont =cont+'<tr><td class=pickup_edit_pic>旧密码</td><td class=pickup_edit_passwd>'+ss+'</td></tr>';
cont =cont+'<tr><td class=pickup_edit_pic>新密码</td><td class=pickup_edit_passwd>'+ss1+'</td></tr>';
cont =cont+'<tr><td class=pickup_edit_pic>确认新密码</td><td class=pickup_edit_passwd>'+ss2+'</td></tr>';
cont =cont+'<tr><td colspan=2 style=text-align:center;padding-top:15px;>'+ss3+'</td></tr></table>';	
 
	} 
  //jQuery('#mdevice_user').css({"float":"left"});
  jQuery('#mdevice_user > td').css({"font-size":"16px"});
  jQuery('#mdevice_user').dialog("open");
  //jQuery('#mdevice_user').popup('open');
	jQuery("#mdevice_user").html(cont);
}

function cancelother(a,b)
{
jQuery('#'+a+'').html(b);
jQuery('#'+a+'button').html('<input class="modify" type=button value=修改 onclick=chgorther("'+a+'","'+b+'")>');
}

function chgorther(a,b)
{
	
	//alert(a);
	if(a=='email')
	 {
 cont='<input class="pickup_edit_password_se" size=30 type=text id=chg'+a+' name=chg'+a+' value='+b+'>';
   }
  else
  	{
 cont='<input class="pickup_edit_password_se" size=30 type=text id=chg'+a+' name=chg'+a+' value='+b+'>'; 		
  	}
 jQuery('#'+a+'').html(cont);
 jQuery('#'+a+'button').html('<input style="color:#ffffff;" class="modify" type=button onclick=changepickup("'+a+'") value=提交>');
   
}

function cancel()
{
	jQuery('#mdevice_user').dialog("close");
}

function delpickup(a,b)
{
	jQuery.ajax({
		type: 'POST',
		url: './config/banksub.php',
		data: 'user=' + a + '&uid=' + b + '&del=pick',		
		    error: function(xhr)
		      {
			   swal({title: "網路忙線中", text: '網路忙線中，請稍後再試!錯誤代碼:1135', confirmButtonText: "确定", customClass: 'twitter'});
			     //jQuery('#order').html('<font color=#FF0000>网路忙线中，请稍后再试!错误代码:681</font>');
			    //alert('網路忙線中，請稍後再試!錯誤代碼:save255');
			    return false;
		      },
			   beforeSend:function(response)
					{   
					}, 
	      success:function(response)
	        {
	       var ss = jQuery.trim(response);
	       var qq = ss.split(":"); 
				  	   if(qq[0]=='000')
				  	   {
				  	   	
				  	   	swal({title: "成功讯息", text: qq[1], confirmButtonText: "确定", customClass: 'twitter'},
				  	   	    function(){
                    document.location.href='./savepickup.php?type=3';
	                  });
				  	   	//change_alert('#00C100','错误讯息',qq[1],'#FFFFFF','s_pick');				  	   	
				  	   }
				  	  else if(qq[0]=='404') 
				  	   {
				  	   swal({title: "错误讯息", text: qq[1], confirmButtonText: "确定",customClass: 'twitter'}); 
				  	   	
				  	   //change_alert('#FF0000','错误讯息',qq[1],'#FFFFFF','s_pick');
				  	   }
					    else
					     {
					     swal({title: "错误讯息", text: qq[1], confirmButtonText: "确定",customClass: 'twitter'}); 
				  	   	
					     //change_alert('#FF0000','错误讯息',qq[1],'#FFFFFF','s_pick');
					     }
			             
		      }
    
	 });
}

/*智付寶*/
function Itelgpay_js()
{
	 var sre = /^[0-9]+(\.[0-9]{0,0})?$/;
	 var amount = jQuery("#amount").val();
	 if(!sre.test(amount))
	    {
	    	swal({title: "错误讯息", text: "存款点数请输入整数！", confirmButtonText: "确定",customClass: 'twitter'});	    	
	    	return;
	    }
	 else if(amount < 100)
	    {
	    	swal({title: "错误讯息", text: "存款点数必须大于或等于100点", confirmButtonText: "确定",customClass: 'twitter'});
	    	return;
	    }
	 else if(amount > 2000)
	    {
	    	swal({title: "错误讯息", text: "存款点数必须小于或等于2000点", confirmButtonText: "确定",customClass: 'twitter'});
	    	return;
	    }
	 else
	 	{
	 	}
	
	document.itelgpay.submit();
}