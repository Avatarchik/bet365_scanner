jQuery(function(){
  
  jQuery( "#savepickupwindow" ).dialog({
			autoOpen: false,
			width: 325,
			height: 400,
			show: "blind",
			hide: "blind",
			modal: false,
			position: [220,150] 
		});
	jQuery( "#savepickupwindow" ).dialog( "option", "buttons", 
	[
    {
        text: "關閉視窗",
        click: function() { 
        jQuery('.ui-dialog').dialog("close");
        jQuery('.ui-dialog').hide();
        window.location.href='./savepickup.php';
        }
        
    }
    
  ]);
		
		jQuery("div[role='dialog'] span.ui-icon-closethick").hide();
		jQuery("div[role='dialog'] a").hide();
		jQuery("div[role='dialog'] div.ui-dialog-titlebar").css({'background':'#234d87'});
	  
 });
 
